{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Run Two KamaCache Nodes",
      "configurations": [
        "NodeA",
        "NodeB"
      ]
    }
  ],
  "configurations": [
    {
      "name": "NodeA",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/example",
      "args": [
        "-port",
        "8001",
        "-node",
        "A",
      ],
      "env": {
        "ETCD_ENDPOINTS": "localhost:2379"
      },
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "NodeB",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/example",
      "args": [
        "-port",
        "8002",
        "-node",
        "B",
      ],
      "env": {
        "ETCD_ENDPOINTS": "localhost:2379"
      },
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}